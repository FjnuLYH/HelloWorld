<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.intent_web_test">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>




        <activity android:name=".SeconActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="org.crazyit.intent.action.CRAZYIT_ACTION"/>
                <action android:name="helloWorld"/>
                <category android:name="org.crazyit.intent.category.CRAZYIT_CATAGORY"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".NewWebActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="org.crazyit.intent.action.BROWSER_ACTION"/>
                <category android:name="org.crazyit.intent.category.BROWSER_CATAGORY"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>




    </application>

</manifest>